<h1><%= t('.header', title: @test.title) %></h1>
<%= link_to t('.edit'), edit_admin_test_path(@test) %>
<h2><%= t('.questions') %></h2>
<table>
  <tr>
    <th><%= t('.question_id') %></th>
    <th><%= t('.question_body') %></th>
    <th colspan="3"><%= t('.actions') %></th>
  </tr>
  <%= render @test.questions, collection: @test.questions %>
</table>
<br>
<%= link_to t('.add_question'), new_admin_test_question_path(@test) %>
